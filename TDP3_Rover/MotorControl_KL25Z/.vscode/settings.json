{
    // Use the compile_commands.json that CMake generates
    "C_Cpp.default.compileCommands": "${workspaceFolder}/cmake_build/KL25Z/develop/GCC_ARM/compile_commands.json",

    // Tell VS Code which compiler and IntelliSense mode to emulate
    "C_Cpp.default.compilerPath": "/usr/bin/arm-none-eabi-gcc",
    "C_Cpp.default.intelliSenseMode": "gcc-arm",

    // Let the CMake Tools extension feed include paths/defines
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools"
}

